<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sports Section</title>
</head>
<body>
    <%- include("header") -%>
  <div class="table-responsive w-50 mx-auto">
        <table class="table">
          <thead>
            <tr>
              <th style="width:60%;">Student Name</th>
              <th class="th-sm">Sport Name</th>
            </tr>
          </thead>
          <% allrecord.forEach(function(record_sp){ %>
          <tbody>
            <tr>
                <td><%= record_sp.Name %></td>
                <td><%=record_sp.Sports%></td>
            </tr>
          </tbody>
          <% }) %>
        </table>      
      </div>
    <%- include("footer") -%>
</body>
</html>